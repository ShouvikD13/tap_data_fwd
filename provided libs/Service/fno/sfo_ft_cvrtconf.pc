/***************** ver 5.5 start*********************/
  if(ptr_st_tradebook->c_prd_typ == SLTP_FUTUREPLUS && (c_fc_flg == 'C' || c_fc_flg == 'T') )
  {
    fn_userlog( c_ServiceName, "Cancel Cover/Profit for fresh ref:%s:",c_frsh_ordr_rfrnc);
