		INTO   :d_ord_lmt_rt
		FROM   FOD_FO_ORDR_DTLS
		WHERE  FOD_ORDR_RFRNC = (
															SELECT min(FOD_ORDR_RFRNC)
															FROM	 FOD_FO_ORDR_DTLS 
															WHERE  FOD_SLTP_ORDR_RFRNC  = :c_sltp_ord_rfrnc
															AND    FOD_FC_FLAG = 'C'
