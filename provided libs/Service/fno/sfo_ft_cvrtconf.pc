		fn_errlog ( c_ServiceName, "S31305", SQLMSG, ptr_st_err_msg->c_err_msg );
		fn_aborttran( c_ServiceName, i_trnsctn, ptr_st_err_msg->c_err_msg );
		tpreturn(TPFAIL, ERR_BFR, (char *)ptr_st_err_msg, 0, 0 );
	 }

	EXEC SQL
