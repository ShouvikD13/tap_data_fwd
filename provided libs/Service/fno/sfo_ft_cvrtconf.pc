                         fn_errlog(c_ServiceName, "S31245", LIBMSG,  ptr_st_err_msg->c_err_msg);
                         tpreturn(TPSUCCESS, SUCC_BFR, NULL ,0, 0 );
           }
       }
       fn_userlog(c_ServiceName," 9. l_sroll_diff_amt :%ld:",l_sroll_diff_amt);

       EXEC SQL
            UPDATE FOD_FO_ORDR_DTLS
