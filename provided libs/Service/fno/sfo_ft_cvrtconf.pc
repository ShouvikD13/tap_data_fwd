  /**** version 5.0 ended ****/

	/*********** Ver 2.1 Starts ********************/

	
	if(ptr_st_tradebook->c_prd_typ == SLTP_FUTUREPLUS )	
	{	
		/*** Commented in Ver 3.2  ***
		EXEC SQL
			SELECT
						fod_sltp_ordr_rfrnc,
