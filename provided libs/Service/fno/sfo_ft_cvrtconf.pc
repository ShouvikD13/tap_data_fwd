      INTO   :cvr_rms_prcsd_flg
      FROM FXB_FO_XCHNG_BOOK
      WHERE FXB_ORDR_RFRNC = :c_cvr_ordr_rfrnc
      AND    FXB_MDFCTN_CNTR = (SELECT min(FXB_MDFCTN_CNTR)
                                FROM   FXB_FO_XCHNG_BOOK
                                WHERE  FXB_ORDR_RFRNC= :c_cvr_ordr_rfrnc
