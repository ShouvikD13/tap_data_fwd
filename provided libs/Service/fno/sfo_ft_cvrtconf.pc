  sprintf(st_pstn_actn.c_ref_rmrks,"%s-T",ptr_st_tradebook->c_trd_rfrnc);
  rtrim(st_pstn_actn.c_ref_rmrks);

	  /*** Ver 5.0 Starts Here ***/

  if(st_orderbook.c_spl_flg == ROLLOVER_WITH_SPREAD )
