        AND     ftd_undrlyng        = :c_undrlyng
        AND     ftd_expry_dt        = :c_expry_dt
        AND     ftd_trd_flw         = 'S';

      if ( SQLCODE != 0 )
      {
        fn_errlog( c_ServiceName, "S31600", SQLMSG, c_errmsg  );
        return -1;
      }

    EXEC SQL ALLOCATE :trd_cursor;

        EXEC SQL  EXECUTE
          BEGIN
            OPEN  :trd_cursor FOR
              SELECT ftd_exctd_qty,
                     ftd_exctd_rt,
                     ftd_trd_dt
              FROM FTD_FO_TRD_DTLS
              WHERE   ftd_clm_mtch_accnt  = :c_cln_mtch_accnt
              AND     ftd_xchng_cd        = :c_xchng_cd
              AND     ftd_prdct_typ = 'U'
              AND     ftd_undrlyng        = :c_undrlyng
              AND     ftd_expry_dt        = :c_expry_dt
              AND     ftd_trd_flw         = 'B'
              ORDER BY ftd_trd_dt;
          END;
        END-EXEC;


      li_bal_qty = l_tot_sell_qty;
		while ( 1 )
    {
        li_qty_bal = 0;

        EXEC SQL
          FETCH :trd_cursor
          INTO :li_exctd_qty,
               :li_exctd_rt,
               :c_trd_dt;

        if ( SQLCODE != 0 )
        {
