  }
	
	EXEC SQL
        SELECT  NVL ( SUM ( ftd_exctd_qty ), 0),
                NVL ( SUM ( ( ftd_exctd_qty * ftd_exctd_rt ) ), 0 )
        INTO    :l_tot_sell_qty,
                :d_sell_val
        FROM    FTD_FO_TRD_DTLS
