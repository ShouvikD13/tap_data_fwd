					AND   fco_prdct_typ				= :c_prdct_typ
					AND   fco_undrlyng        = :c_undrlyng
					AND   fco_expry_dt        = :c_expry_dt
					AND   fco_exer_typ        = :c_exrc_typ
					AND   fco_ordr_flw				= 'S'
