    }else{
     if( (c_run_mode == EOD_MODE ) && ( d_balance_amt != 0 ) )
     {
/**
     if( fn_get_free_limit(c_ServiceName,ptr_st_pstn_actn->c_cln_mtch_accnt,&d_free_amount,&d_amt,ptr_st_pstn_actn->c_xchng_cd,c_err_msg) == -1 )
