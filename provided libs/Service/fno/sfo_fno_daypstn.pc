																	&d_min_mrgn_prcnt,
																	&d_min_spread_mrgn,
																	c_prdct_typ,
																	c_xchng_cd
																); 

  if(i_fn_stts== -1)
  {
    return -1;
  }

	if( DEBUG_MSG_LVL_3 )
	{
		fn_userlog(c_ServiceName,"c_mtch_accnt :%s:",c_mtch_accnt);
