				FROM
				(
				select sum(tradevalue) tradevalue , undrlyng
				FROM
				(
				select  
					sum(FOD_STRK_PRC *  (( FOD_ORDR_TOT_QTY - (FOD_EXEC_QTY - FOD_CNCL_QTY - FOD_EXPRD_QTY) ) + FOD_EXEC_QTY )) tradevalue ,
					FOD_UNDRLYNG undrlyng
